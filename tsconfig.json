{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "outDir": "dist/main",
    "jsx": "react-jsx",
    "lib": ["esnext", "DOM"],
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
